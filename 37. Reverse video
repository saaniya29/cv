import cv2
cap = cv2.VideoCapture(r"C:\Users\Saaniya\Pictures\Screenshots\.2) vehicle vid.mp4")
total = int(cap.get(cv2.CAP_PROP_FRAME_COUNT))
current = total - 1
while current>= 0:
    cap.set(cv2.CAP_PROP_POS_FRAMES, current)
    ret, frame = cap.read()
    if not ret:
        break
    cv2.imshow('Video in Rev', frame)
    if cv2.waitKey(40) & 0xFF == ord('q'):
        break
    current -= 1
cap.release()
cv2.destroyAllWindows()
