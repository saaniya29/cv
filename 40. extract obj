import cv2
import matplotlib.pyplot as plt
img = cv2.imread(r"C:\Users\Saaniya\Pictures\Screenshots\images.jpg")
x, y, w, h = 100, 50, 200, 150
cv2.rectangle(img, (x, y), (x + w, y + h), (0, 255, 0), 2)
exobject = img[y:y + h, x:x + w]
img_rgb = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)
exobject_rgb = cv2.cvtColor(exobject, cv2.COLOR_BGR2RGB)
plt.imshow(exobject_rgb)
plt.title('Extracted Object')
plt.show()
